<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <PackageId>Genocs.Library.Demo.HelloWorld.WebApi</PackageId>
        <RootNamespace>Genocs.Library.Demo.HelloWorld.WebApi</RootNamespace>
        <AssemblyName>Genocs.Library.Demo.HelloWorld.WebApi</AssemblyName>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>Genocs</UserSecretsId>
        <IsPublishable>false</IsPublishable>
        <IsPackable>false</IsPackable>
    </PropertyGroup>

    <ItemGroup Condition="'$(Configuration)' == 'Debug'">
        <ProjectReference Include="..\..\Genocs.Auth\Genocs.Auth.csproj" />
        <ProjectReference Include="..\..\Genocs.Logging\Genocs.Logging.csproj" />
        <ProjectReference Include="..\..\Genocs.Telemetry\Genocs.Telemetry.csproj" />
        <ProjectReference Include="..\..\Genocs.Saga\Genocs.Saga.csproj" />
        <ProjectReference Include="..\..\Genocs.WebApi.Swagger\Genocs.WebApi.Swagger.csproj" />
    </ItemGroup>

    <ItemGroup Condition="'$(Configuration)' == 'Release'">
        <PackageReference Include="Genocs.Auth" Version="8.1.*" />
        <PackageReference Include="Genocs.Logging" Version="8.1.*" />
        <PackageReference Include="Genocs.Telemetry" Version="8.1.*" />
        <PackageReference Include="Genocs.Saga" Version="9.0.*" />
        <PackageReference Include="Genocs.WebApi.Swagger" Version="8.1.*" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Infrastructure\Infrastructure.csproj" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Extensions\" />
    </ItemGroup>
    
</Project>
