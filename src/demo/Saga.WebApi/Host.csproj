<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net10.0</TargetFramework>
        <PackageId>Genocs.Library.Demo.Saga.WebApi</PackageId>
        <RootNamespace>Genocs.Library.Demo.Saga.WebApi</RootNamespace>
        <AssemblyName>Genocs.Library.Demo.Saga.WebApi</AssemblyName>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>Genocs</UserSecretsId>
        <IsPublishable>false</IsPublishable>
        <IsPackable>false</IsPackable>
    </PropertyGroup>

    <ItemGroup Condition="'$(Configuration)' == 'Debug'">
        <ProjectReference Include="..\..\Genocs.Logging\Genocs.Logging.csproj" />
        <ProjectReference Include="..\..\Genocs.Saga\Genocs.Saga.csproj" />
    </ItemGroup>

    <ItemGroup Condition="'$(Configuration)' == 'Release'">
        <PackageReference Include="Genocs.Logging" Version="8.1.*" />
        <PackageReference Include="Genocs.Saga" Version="9.0.*" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Infrastructure\Infrastructure.csproj" />
    </ItemGroup>

</Project>
