<Project Sdk="Microsoft.NET.Sdk.Web">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <UserSecretsId>genocs</UserSecretsId>
        <IsPublishable>false</IsPublishable>
        <IsPackable>false</IsPackable>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="MassTransit.RabbitMQ" Version="8.4.0" />
    </ItemGroup>

    <ItemGroup Condition="'$(Configuration)' == 'Debug'">
        <ProjectReference Include="..\..\Genocs.Auth\Genocs.Auth.csproj" />
        <ProjectReference Include="..\..\Genocs.Logging\Genocs.Logging.csproj" />
        <ProjectReference Include="..\..\Genocs.Tracing\Genocs.Tracing.csproj" />
        <ProjectReference Include="..\..\Genocs.HTTP\Genocs.HTTP.csproj" />
        <ProjectReference Include="..\..\Genocs.Security\Genocs.Security.csproj" />
        <ProjectReference Include="..\..\Genocs.WebApi.Swagger\Genocs.WebApi.Swagger.csproj" />
        <ProjectReference Include="..\..\Genocs.WebApi.Security\Genocs.WebApi.Security.csproj" />
        <ProjectReference Include="..\..\Genocs.Secrets.AzureKeyVault\Genocs.Secrets.AzureKeyVault.csproj" />
        <ProjectReference Include="..\..\Genocs.Metrics\Genocs.Metrics.csproj" />
    </ItemGroup>

    <ItemGroup Condition="'$(Configuration)' != 'Debug'">
        <PackageReference Include="Genocs.Auth" Version="7.4.*" />
        <PackageReference Include="Genocs.Logging" Version="7.4.*" />
        <PackageReference Include="Genocs.Tracing" Version="7.4.*" />
        <PackageReference Include="Genocs.HTTP" Version="7.4.*" />
        <PackageReference Include="Genocs.Security" Version="7.4.*" />
        <PackageReference Include="Genocs.WebApi.Swagger" Version="7.4.*" />
        <PackageReference Include="Genocs.WebApi.Security" Version="7.4.*" />
        <PackageReference Include="Genocs.Secrets.AzureKeyVault" Version="7.4.*" />
        <PackageReference Include="Genocs.Metrics" Version="7.4.*" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\Genocs.Core.Demo.Contracts\Genocs.Core.Demo.Contracts.csproj" />
        <ProjectReference Include="..\Genocs.Core.Demo.Domain\Genocs.Core.Demo.Domain.csproj" />
    </ItemGroup>

    <ItemGroup>
        <Folder Include="logs\" />
    </ItemGroup>

</Project>
