{
  "app": {
    "name": "Demo WebApi",
    "service": "demo_webapi",
    "instance": "1",
    "version": "v1.0",
    "displayBanner": true,
    "displayVersion": true
  },
  "logger": {
    "level": "debug",
    "excludePaths": [
      "/",
      "/healthz",
      "/alive",
      "/metrics"
    ],
    "excludeProperties": [
      "api_key",
      "access_key",
      "ApiKey",
      "ApiSecret",
      "ClientId",
      "ClientSecret",
      "ConnectionString",
      "Password",
      "Email",
      "Login",
      "Secret",
      "Token"
    ],
    "console": {
      "enabled": true,
      "enableTracing": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "tags": {}
  },
  "jaeger": {
    "enabled": true,
    "serviceName": "users",
    "udpHost": "localhost",
    "udpPort": 6831,
    "maxPacketSize": 65000,
    "sampler": "const",
    "excludePaths": [
      "/",
      "/ping",
      "/metrics"
    ]
  },
  "jwt": {
    "enabled": true,
    "_ValidIssuer": "http://localhost/Auth",
    "_ValidAudience": "https://localhost:5000",
    "_Secret": "",
    "issuer": "https://securetoken.google.com/fiscanner-web",
    "audience": "fiscanner-web",
    "requireHttpsMetadata": true
  },
  "firebaseAuthorization": {
    "defaultRoles": [
      "User"
    ],
    "emailDomainRoles": {
      "@genocs.com": [
        "Admin",
        "Editor",
        "User"
      ]
    },
    "emailRoles": {
      "giovanni.nocco@hotmail.com": [
        "Editor",
        "User"
      ]
    },
    "requireEmailVerification": false,
    "demoMode": true
  },
  "rabbitMQ": {
    "HostName": "localhost",
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest"
  },
  "mongoDb": {
    "connectionString": "mongodb://localhost:27017",
    "database": "genocslibrarytemplate-service",
    "enableTracing": true,
    "seed": false
  },
  "databaseSettings": {
    "DBProvider": "mongodb",
    "ConnectionString": "mongodb://localhost:27017"
  },
  "azureKeyVault": {
    "enabled": false,
    "name": "kv-genocs"
  },
  "secrets": {
    "Secret": "This is a secret coming from the appsettings.json file"
  },
  "httpClient": {
    "type": "",
    "retries": 2,
    "services": {
      "ca_issuer": "<<>>"
    }
  },
  "externalService": {
    "caller": "<<>>",
    "public": "<<>>",
    "private": "<<>>"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": true,
    "name": "DemoWebApi",
    "title": "Demo WebApi",
    "version": "v2",
    "description": "The Demo API.",
    "routePrefix": "swagger",
    "contactName": "Giovanni Nocco",
    "contactEmail": "giovanni.nocco@genocs.com",
    "contactUrl": "https://www.genocs.com",
    "licenseName": "Use under MIT",
    "licenseUrl": "https://opensource.org/license/mit",
    "termsOfService": "https://www.genocs.com/terms_and_conditions.html",
    "includeSecurity": true,
    "servers": [
      {
        "url": "https://fiscanner-api.azurewebsites.net",
        "description": "Production deployed on Azure"
      },
      {
        "url": "http://localhost:5300",
        "description": "Local version to be used for development"
      },
      {
        "url": "http://fiscanner-api",
        "description": "Containerized version to be used into with docker or k8s"
      }
    ]
  },
  "metrics": {
    "enabled": true,
    "influxEnabled": false,
    "prometheusEnabled": false,
    "influxUrl": "http://localhost:8086",
    "influxDatabase": "demo",
    "influxUsername": "admin",
    "influxPassword": "yourpassword",
    "interval": 5
  },
  "prometheus": {
    "enabled": false,
    "endpoint": "http://localhost:5070"
  },
  "AllowedHosts": "*",
  "authorization": {
    "enabled": true,
    "devApiKey": "abc_123",
    "apiKeys": [
      "prod_api_key_1",
      "prod_api_key_2"
    ]
  }
}